@using Net.Qks
@using Net.Qks.Web.Areas.Admin.Models.Layout
@using Net.Qks.Web.Areas.Admin.Views.Shared.Components.AdminLogo
@model FooterViewModel
@{
    Layout = null;
}
<div class="kt-footer  kt-footer--extended  kt-grid__item" id="kt_footer">
    <div class="kt-footer__bottom">
        <div class="kt-container">
            <div class="kt-footer__wrapper">
                <div class="kt-footer__copyright">
                    @Model.GetProductNameWithEdition() | v@(AppVersionHelper.Version) [@AppVersionHelper.ReleaseDate.ToString("yyyyMMdd")]
                </div>
                <div class="kt-footer__menu">
                </div>
            </div>
        </div>
    </div>
</div>
